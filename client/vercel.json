{
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "https://shopkart-server-ten.vercel.app/api/$1"
    },
    {
      "source": "/uploads/(.*)",
      "destination": "https://shopkart-server-ten.vercel.app/uploads/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}